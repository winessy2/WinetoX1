	Name = üïä –í–∫–ª—é—á–∏—Ç—å Fly Bypass (XNEOFF),
	Callback = function()
		loadstring(gameHttpGet(httpsraw.githubusercontent.comXNEOFFFlyGuiV3mainFlyGuiV3.txt))()
	end
})

-- === Infinite Jump ===
UIS.JumpRequestConnect(function()
	if infJumpEnabled then
		local h = player.Character and player.CharacterFindFirstChildOfClass(Humanoid)
		if h then
			hChangeState(Enum.HumanoidStateType.Jumping)
		end
	end
end)

mainTabCreateToggle({
	Name = üîÅ –í–∫–ª—é—á–∏—Ç—å Infinite Jump,
	CurrentValue = false,
	Flag = InfJumpToggle,
	Callback = function(state)
		infJumpEnabled = state
	end
})

-- === Prompt Bypass ===
mainTabCreateButton({
	Name = ‚ö° –£–±—Ä–∞—Ç—å –∑–∞–¥–µ—Ä–∂–∫—É —É –≤—Å–µ—Ö ProximityPrompt,
	Callback = function()
		local function bypassPrompt(prompt)
			if promptIsA(ProximityPrompt) then
				prompt.HoldDuration = 0
				prompt.RequiresLineOfSight = false
			end
		end

		for _, obj in ipairs(workspaceGetDescendants()) do
			bypassPrompt(obj)
		end

		workspace.DescendantAddedConnect(function(obj)
			bypassPrompt(obj)
		end)

		RayfieldNotify({
			Title = –ì–æ—Ç–æ–≤–æ!,
			Content = –í—Å–µ ProximityPrompt —Ç–µ–ø–µ—Ä—å –º–æ–º–µ–Ω—Ç–∞–ª—å–Ω—ã–µ.,
			Duration = 4
		})
	end
})

-- === üîí Lock Monitor (–ø–æ Timer = 0s) ===
local lockBaseEnabled = false

mainTabCreateToggle({
	Name = üîí –ê–≤—Ç–æ Lock Base (–ø–æ —Ç–∞–π–º–µ—Ä—É),
	CurrentValue = false,
	Flag = LockBaseToggle,
	Callback = function(state)
		lockBaseEnabled = state
		RayfieldNotify({
			Title = Lock Base,
			Content = state and –ê–≤—Ç–æ Lock –≤–∫–ª—é—á–µ–Ω or –ê–≤—Ç–æ Lock –≤—ã–∫–ª—é—á–µ–Ω,
			Duration = 4
		})
	end
})
task.spawn(function()
	while true do
		if lockBaseEnabled then
			local base = FindPlayerBase()
			local root = player.Character and player.CharacterFindFirstChild(HumanoidRootPart)

			if base and root then
				local lock = baseFindFirstChild(Lock, true)
				if lock and lockIsA(BasePart) then
					local timer = lockFindFirstChild(LockAttachment, true)
					if timer then
						timer = timerFindFirstChild(LockGui, true)
						if timer then
							timer = timerFindFirstChild(Timer, true)
							if timer and timerIsA(TextLabel) then
								if timer.Text == 0s then
									local originalCFrame = lock.CFrame

									lock.CFrame = root.CFrame
									lock.CanCollide = false
									lock.Transparency = 1

									RayfieldNotify({
										Title = Lock –∞–≤—Ç–æ–∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω,
										Content = Lock –ø–µ—Ä–µ–Ω–µ—Å—ë–Ω –∫ —Ç–µ–±–µ –Ω–∞ 3 —Å–µ–∫—É–Ω–¥—ã.,
										Duration = 3
									})

									task.wait(0.5)

									lock.CFrame = originalCFrame
									lock.CanCollide = true
									lock.Transparency = 0

									RayfieldNotify({
										Title = Lock –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω,
										Content = Lock –≤–µ—Ä–Ω—É–ª—Å—è –Ω–∞ –º–µ—Å—Ç–æ.,
										Duration = 3
									})

									-- –ü–æ–¥–æ–∂–¥—ë–º –Ω–µ–º–Ω–æ–≥–æ, —á—Ç–æ–±—ã –Ω–µ —Å—Ä–∞–±–æ—Ç–∞–ª–æ –ø–æ–≤—Ç–æ—Ä–Ω–æ
									task.wait(1.5)
								end
							end
						end
					end
				end
			end
		end
		task.wait(0.2)
	end
end)


-- === üí∏ Instant Steal ===
mainTabCreateButton({
	Name = üí∏ Instant Steal (CollectZone –∫ —Ç–µ–±–µ),
	Callback = function()
		local base = FindPlayerBase()
		local root = player.Character and player.CharacterFindFirstChild(HumanoidRootPart)

		if base and root then
			local cz = baseFindFirstChild(CollectZone, true)
			if cz and czIsA(BasePart) then
				local original = cz.CFrame
				cz.CFrame = root.CFrame
				cz.Transparency = 1
				cz.CanCollide = false
				task.wait(0.1)
				cz.CFrame = original
				cz.Transparency = 0
				cz.CanCollide = true
			end
		end
	end
})
